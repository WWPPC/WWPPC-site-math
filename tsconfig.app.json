{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "WWPPC-site-common/src/**/*"
    ],
    "exclude": [],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "#/*": [
                "./WWPPC-site-common/src/*"
            ]
        },
        "types": [
            "node"
        ]
    }
}